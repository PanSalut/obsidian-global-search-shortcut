{
  "name": "obsidian-global-search",
  "version": "1.0.0",
  "description": "Obsidian plugin with system-wide hotkey for searching notes with live preview",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "global-hotkey",
    "search",
    "preview"
  ],
  "author": "PanSalut <artur@fabrykawitryn.pl>",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^20.11.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.19.12",
    "obsidian": "latest",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "fuse.js": "^7.1.0"
  }
}
